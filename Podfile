project 'EasyDriving.xcodeproj'

# Uncomment the next line to define a global platform for your project
platform :ios, '12.0'

target 'EasyDriving' do
    # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
    use_frameworks!
    #  source 'https://github.com/CocoaPods/Specs.git'
    
    #https://github.com/CosmicMind/Material
    pod 'Material'

    # 下拉刷新 https://github.com/CoderMJLee/MJRefresh
    pod 'MJRefresh'

    # material progres https://github.com/misterwell/MMMaterialDesignSpinner
    pod 'MMMaterialDesignSpinner'
    
    #https://github.com/bfeher/BFPaperButton
    # pod 'BFPaperButton'
    
    #自动布局 https://github.com/cloudkite/Masonry
    pod 'Masonry'

    # 图标库  https://github.com/PrideChung/FontAwesomeKit
    pod 'FontAwesomeKit', '~> 2.2.0'

    # 调视图控件 https://github.com/adad184/MMPlaceHolder
    pod 'MMPlaceHolder'
    
    # AFNetworking https://github.com/AFNetworking/AFNetworking
    pod 'AFNetworking', '~> 3.0'

    # 异步加载图片、缓存等 https://github.com/rs/SDWebImage.git
    pod 'SDWebImage', '~>4.0'

    # 网络状态监测 https://github.com/tonymillion/Reachability.git
   pod 'Reachability'
    
    # 下滑提示 https://github.com/cwRichardKim/RKDropdownAlert
    pod 'RKDropdownAlert'

    # 加载提示 https://github.com/matej/MBProgressHUD.git
    pod 'MBProgressHUD'

    # Material TableView https://github.com/bfeher/BFPaperTableViewCell
    pod 'BFPaperTableViewCell'

    # 下拉菜单 https://github.com/AssistoLab/DropDown
    pod 'DropDown'

    pod 'ChameleonFramework'
    
    # 调视图控件 https://github.com/adad184/MMPlaceHolder
    pod 'MMPlaceHolder'
    
    # 高德地图
    pod 'AMapSearch'
    pod 'AMapNavi'
    pod 'AMapLocation'
    
end

